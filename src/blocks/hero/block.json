		"mediaOverflow": {
			"type": "boolean"
		},
		"verticalContentAlignment": {
			"type": "string"
		},
		"imageFill": {
			"type": "boolean"
		},
		"contentGridLineStart": {
			"type": "number"
		},
		"contentGridLineEnd": {
			"type": "number"
		},
		"mediaGridLineStart": {
			"type": "number"
