{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "ainoblocks/timeline",
  "version": "1.0.0",
  "title": "Timeline",
  "description": "Aino time line block description.",
  "category": "ainoblocks",
  "keywords": [
    "timeline",
    "CV",
    "about",
    "event",
    "aino",
    "ainoblocks"
  ],
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "eventPosition": {
      "type": "string",
      "default": "right"
    },
    "eventOverlap": {
      "type": "boolean",
      "default": false
    },
    "markerPosition": {
      "type": "string"
    },
    "markerStyle": {
      "type": "string"
    },
    "eventLineTop": {
      "type": "string"
    },
    "eventLineBottom": {
      "type": "string"
    }
  },
  "supports": {
    "align": [
      "wide"
    ],
    "anchor": true,
    "__experimentalBorder": {
      "color": true,
      "radius": false,
      "style": false,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "width": true
      }
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "__experimentalDefaultControls": {
        "padding": true,
        "margin": true
      }
    }
  }
}